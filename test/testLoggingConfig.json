{
    "version": 1,
    "disable_existing_loggers": "False",
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        },
        "testSimple": {
            "format": "%(levelname)s: %(module)s: %(message)s"
        }
    },
    "handlers": {
        "consoleHandler": {
            "class": "logging.StreamHandler",
            "formatter": "testSimple",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "root": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "TestChip8": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "TestChip8Registers": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "TestChip8KeyPad": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "org.hasii.chip8.Chip8": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        }

    }
}
